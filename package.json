{
  "name": "n8n-nodes-kv-storage",
  "version": "0.0.1",
  "description": "Key-Value Storage Node for n8n supporting different scopes",
  "keywords": [
    "key-value",
    "storage",
    "in-memory",
    "cache",
    "scope",
    "trigger",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://telepilot.co/nodes/kv-storage",
  "author": {
    "name": "TelePilot",
    "email": "contact@telepilot.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telepilotco/n8n-nodes-kv-storage.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "tslint -p tsconfig.json -c tslint.json && eslint nodes credentials package.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json && eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "nodes": [
      "dist/nodes/KVStorage/KVStorage.node.js",
      "dist/nodes/KVStorage/KVStorageTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.14.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "gulp": "^4.0.2",
    "n8n-core": "^0.161.0",
    "n8n-workflow": "^0.143.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "typedi": "^0.10.0"
  }
}
