{
  "name": "n8n-nodes-kv-storage",
  "version": "0.3.4",
  "description": "Key-Value Storage Node for n8n supporting different scopes with list operations",
  "keywords": [
    "key-value",
    "storage",
    "in-memory",
    "cache",
    "scope",
    "trigger",
    "list",
    "array",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://nguyenngothuong.com",
  "author": {
    "name": "Nguyen Ngo Thuong",
    "email": "support@nguyenngothuong.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyenngothuong/n8n-nodes-kv-storage.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "tslint -p tsconfig.json -c tslint.json && eslint nodes package.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json && eslint nodes package.json --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/KVStorage/KvStorage.node.js",
      "dist/nodes/KVStorage/KvStorageTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.14.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint": "8",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.161.0",
    "n8n-workflow": "^0.143.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "typedi": "^0.10.0"
  },
  "bugs": {
    "url": "https://github.com/nguyenngothuong/n8n-nodes-kv-storage/issues"
  }
}
